@section css{
    <link rel="stylesheet" type="text/css" href="/Scripts/player1078/player.css" media="all" />
}
@section js{
    <script type="text/javascript">
        var id = '@ViewContext.HttpContext.Request.Query["id"]';
        var vehicleId = '@ViewContext.HttpContext.Request.Query["vid"]';
        var vehicleNumber = '@Html.Raw(ViewContext.HttpContext.Request.Query["vnumber"])';
        var channelId = '@ViewContext.HttpContext.Request.Query["cid"]';
        var w = '@ViewContext.HttpContext.Request.Query["wt"]';
        var h = '@ViewContext.HttpContext.Request.Query["ht"]';
        var bgColor = '@ViewContext.HttpContext.Request.Query["bgColor"]';
        var videoUrl = '@ViewContext.HttpContext.Request.Query["url"]';
        var avStartTime = '@ViewContext.HttpContext.Request.Query["startTime"]';
        var bitStreamType = '@ViewContext.HttpContext.Request.Query["bst"]';
        var avFlag = '@ViewContext.HttpContext.Request.Query["avFlag"]';
        var muted = '@ViewContext.HttpContext.Request.Query["muted"]' == "true";
        var subtitleDisplay = '@ViewContext.HttpContext.Request.Query["sdp"]';
        var beforeVolume = muted ? 0 : 1.0;
        var playback = '@ViewContext.HttpContext.Request.Query["playback"]' == "true";
        var usingMicrophone = '@ViewContext.HttpContext.Request.Query["ump"]' == "true";
        var usingControlBar = '@ViewContext.HttpContext.Request.Query["vcb"]' != "false";
        var bufferTime = '@(ViewContext.HttpContext.Request.Query["bufferTime"].FirstOrDefault() ?? "3")';
        var liveAppName = '@GPSChina.Platform.Business.Utility.ConfigurationUtility.VideoAppName';
        var videoId = id + '_video';
    </script>
    <script type="text/javascript" src="@HtmlUtility.Content(Url, "~/Scripts/player1078/jvp.js")"></script>>
    <script type="text/javascript" src="@HtmlUtility.Content(Url,"~/Scripts/player1078/player.js")"></script>
}
